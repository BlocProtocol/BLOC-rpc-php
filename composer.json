{
  "name": "turtlecoin/turtlecoin-rpc-php",
  "description": "A PHP wrapper for TurtleCoin's RPC interfaces",
  "keywords": [
    "TurtleCoin",
    "PHP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tim McLeod",
      "email": "tim@timmcleod.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "^6.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "TurtleCoin\\": "src/TurtleCoin"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
